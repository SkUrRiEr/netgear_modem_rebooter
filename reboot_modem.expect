#! /usr/bin/expect -f

set ip [lindex $argv 0];

spawn telnet $ip
set timeout 10
expect {
	timeout {puts "Telnet was not successfully unlocked. Please check configuration."; exit}
	"Unable to connect" {puts "Telnet connection refused. Please reboot manually."; exit}
	"#"
}
send "reboot\r"
expect eof
